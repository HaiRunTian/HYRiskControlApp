<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical">


    <RelativeLayout
        android:id="@+id/titleBarLayout"
        style="@style/TitleBarStyle">

        <TextView
            android:id="@+id/tvBack"
            style="@style/TitleBarStyleLeftText2"
            android:drawableLeft="@mipmap/ic_btn_return"
            />

        <TextView
            android:id="@+id/tvTitles"
            style="@style/TitleBarStyleMiddleText"
            android:text="整改通知查询"/>

    </RelativeLayout>

    <include layout="@layout/layout_submit_man"></include>

    <include layout="@layout/layout_startdate"></include>

    <include layout="@layout/layout_enddate"></include>

    <include layout="@layout/layout_notify_type"></include>



    <include layout="@layout/layout_log_state"></include>
    <include layout="@layout/layout_notify_state"></include>


    <include layout="@layout/layout_query_notify_btn"></include>



</LinearLayout>
